{
  "name": "steem-smartvotes",
  "version": "0.13.2",
  "description": "Node/web library for Smartvotes: a vote delegation system for STEEM blockchain",
  "main": "dist/steem-smartvotes.js",
  "types": "dist/steem-smartvotes.d.ts",
  "directories": {},
  "engines": {
    "node": ">=9.11"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.20",
    "ajv": "^6.4.0",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "jayson": "^2.0.5",
    "setprototypeof": "^1.1.0",
    "steem": "^0.7.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.11",
    "chai": "^4.1.2",
    "json-loader": "^0.5.7",
    "mocha": "^5.1.1",
    "ts-node": "^6.0.2",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "typescript-json-schema": "^0.23.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "scripts": {
    "build": "rm -rf dist && npm run build-node && npm run build-browser",
    "build-node": "npm run build-ts && npm run tslint && npm run build-schema",
    "build-ts": "tsc",
    "build-schema": "npm run convert-schema-to-json",
    "convert-schema-to-json": "typescript-json-schema --strictNullChecks --required --out \"smartvotes.schema.json\" \"src/schema/*.schema.ts\" \"smartvotes_operation\" && cp smartvotes.schema.json dist/schema/smartvotes.schema.json",
    "build-browser": "rm -rf dist/browser/ && NODE_ENV=production webpack --config webpack.config.js",
    "build-browser-dev": "rm -rf dist/browser/ && NODE_ENV=development webpack --config webpack.config.js",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noisy-witness/steem-smartvotes-core.git"
  },
  "keywords": [
    "steem",
    "blockchain",
    "smartvotes",
    "cli"
  ],
  "author": "JÄ™drzej Lewandowski <jedrzejblew@gmail.com> (https://jedrzej.lewandowski.doctor/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noisy-witness/steem-smartvotes-core/issues"
  },
  "homepage": "https://github.com/noisy-witness/steem-smartvotes-core#readme"
}
