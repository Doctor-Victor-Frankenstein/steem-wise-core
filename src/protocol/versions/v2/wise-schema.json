{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "anyOf": [
        {
            "additionalItems": {
                "anyOf": [
                    {
                        "enum": [
                            "v2:send_voteorder"
                        ],
                        "type": "string"
                    },
                    {
                        "$ref": "#/definitions/wise_send_voteorder"
                    }
                ]
            },
            "items": [
                {
                    "enum": [
                        "v2:send_voteorder"
                    ],
                    "type": "string"
                },
                {
                    "$ref": "#/definitions/wise_send_voteorder"
                }
            ],
            "minItems": 2,
            "type": "array"
        },
        {
            "additionalItems": {
                "anyOf": [
                    {
                        "enum": [
                            "v2:set_rules"
                        ],
                        "type": "string"
                    },
                    {
                        "$ref": "#/definitions/wise_set_rules"
                    }
                ]
            },
            "items": [
                {
                    "enum": [
                        "v2:set_rules"
                    ],
                    "type": "string"
                },
                {
                    "$ref": "#/definitions/wise_set_rules"
                }
            ],
            "minItems": 2,
            "type": "array"
        },
        {
            "additionalItems": {
                "anyOf": [
                    {
                        "enum": [
                            "v2:confirm_vote"
                        ],
                        "type": "string"
                    },
                    {
                        "$ref": "#/definitions/wise_confirm_vote"
                    }
                ]
            },
            "items": [
                {
                    "enum": [
                        "v2:confirm_vote"
                    ],
                    "type": "string"
                },
                {
                    "$ref": "#/definitions/wise_confirm_vote"
                }
            ],
            "minItems": 2,
            "type": "array"
        }
    ],
    "definitions": {
        "wise_confirm_vote": {
            "properties": {
                "accepted": {
                    "type": "boolean"
                },
                "msg": {
                    "type": "string"
                },
                "tx_id": {
                    "type": "string"
                },
                "voter": {
                    "type": "string"
                }
            },
            "required": [
                "accepted",
                "msg",
                "tx_id",
                "voter"
            ],
            "type": "object"
        },
        "wise_rule_authors": {
            "properties": {
                "authors": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "mode": {
                    "$ref": "#/definitions/wise_rule_authors_mode"
                },
                "rule": {
                    "enum": [
                        "authors"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "authors",
                "mode",
                "rule"
            ],
            "type": "object"
        },
        "wise_rule_authors_mode": {
            "enum": [
                "allow",
                "deny"
            ],
            "type": "string"
        },
        "wise_rule_custom_rpc": {
            "properties": {
                "host": {
                    "type": "string"
                },
                "method": {
                    "type": "string"
                },
                "path": {
                    "type": "string"
                },
                "port": {
                    "type": "number"
                },
                "rule": {
                    "enum": [
                        "custom_rpc"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "host",
                "method",
                "path",
                "port",
                "rule"
            ],
            "type": "object"
        },
        "wise_rule_tags": {
            "properties": {
                "mode": {
                    "$ref": "#/definitions/wise_rule_tags_mode"
                },
                "rule": {
                    "enum": [
                        "tags"
                    ],
                    "type": "string"
                },
                "tags": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "required": [
                "mode",
                "rule",
                "tags"
            ],
            "type": "object"
        },
        "wise_rule_tags_mode": {
            "enum": [
                "allow",
                "any",
                "deny",
                "require"
            ],
            "type": "string"
        },
        "wise_rule_voting_power": {
            "description": "This rule checks vorting_power of the delegator. Available modes are: more_than, less_than and egual.",
            "properties": {
                "mode": {
                    "$ref": "#/definitions/wise_rule_voting_power_mode"
                },
                "rule": {
                    "enum": [
                        "voting_power"
                    ],
                    "type": "string"
                },
                "value": {
                    "description": "Voting power of the delegator in steem_percent. 100% = 10'000 and 0% = 0. Percentage is multiplied by 100.",
                    "type": "number"
                }
            },
            "required": [
                "mode",
                "rule",
                "value"
            ],
            "type": "object"
        },
        "wise_rule_voting_power_mode": {
            "enum": [
                "equal",
                "less_than",
                "more_than"
            ],
            "type": "string"
        },
        "wise_rule_weight": {
            "properties": {
                "max": {
                    "description": "Maximal upvote weight (0 = disable upvote, 10000 = full upvote)",
                    "maximum": 10000,
                    "minimum": 0,
                    "type": "integer"
                },
                "min": {
                    "description": "Minimal flag weight (-10000 = full flag, 0 = disable flag)",
                    "maximum": 0,
                    "minimum": -10000,
                    "type": "integer"
                },
                "mode": {
                    "type": "string"
                },
                "rule": {
                    "enum": [
                        "weight"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "max",
                "min",
                "rule"
            ],
            "type": "object"
        },
        "wise_send_voteorder": {
            "properties": {
                "author": {
                    "type": "string"
                },
                "delegator": {
                    "type": "string"
                },
                "permlink": {
                    "type": "string"
                },
                "ruleset": {
                    "type": "string"
                },
                "weight": {
                    "description": "Vote / flag weight",
                    "maximum": 10000,
                    "minimum": -10000,
                    "type": "integer"
                }
            },
            "required": [
                "author",
                "delegator",
                "permlink",
                "ruleset",
                "weight"
            ],
            "type": "object"
        },
        "wise_set_rules": {
            "properties": {
                "description": {
                    "type": "string"
                },
                "rulesets": {
                    "items": {
                        "additionalItems": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "items": {
                                        "anyOf": [
                                            {
                                                "$ref": "#/definitions/wise_rule_weight"
                                            },
                                            {
                                                "$ref": "#/definitions/wise_rule_tags"
                                            },
                                            {
                                                "$ref": "#/definitions/wise_rule_authors"
                                            },
                                            {
                                                "$ref": "#/definitions/wise_rule_voting_power"
                                            },
                                            {
                                                "$ref": "#/definitions/wise_rule_custom_rpc"
                                            }
                                        ]
                                    },
                                    "type": "array"
                                }
                            ]
                        },
                        "items": [
                            {
                                "type": "string"
                            },
                            {
                                "items": {
                                    "anyOf": [
                                        {
                                            "$ref": "#/definitions/wise_rule_weight"
                                        },
                                        {
                                            "$ref": "#/definitions/wise_rule_tags"
                                        },
                                        {
                                            "$ref": "#/definitions/wise_rule_authors"
                                        },
                                        {
                                            "$ref": "#/definitions/wise_rule_voting_power"
                                        },
                                        {
                                            "$ref": "#/definitions/wise_rule_custom_rpc"
                                        }
                                    ]
                                },
                                "type": "array"
                            }
                        ],
                        "minItems": 2,
                        "type": "array"
                    },
                    "type": "array"
                },
                "voter": {
                    "type": "string"
                }
            },
            "required": [
                "rulesets",
                "voter"
            ],
            "type": "object"
        }
    }
}

